{
  "name": "e42-companion",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npm install -g eas-cli && npm install --global @expo/ngrok@^4.1.0",
    "lint": "expo lint",
    "clean": "npx rimraf node_modules && npx rimraf package-lock.json && npx rimraf .expo && npx rimraf android && npx rimraf ios && npx rimraf web",
    "c:i": "npm run clean && npm install",
    "go:android": "expo start --android",
    "go:ios": "expo start --ios",
    "go:web": "expo start --web",
    "rebuild": "npx expo prebuild --clean",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "secrets": "node ./scripts/add-secrets-to-build.js",
    "build:a:c:prod": "npm run secrets && npx eas build -p android --profile production",
    "build:a:c:dev": "npm run secrets && npx eas build -p android --profile development",
    "start": "expo start",
    "start:tunnel": "npx expo start --tunnel",
    "start:build": "npx expo start --dev-client",
    "start:build:tunnel": "npm run start:build -- --tunnel"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/native": "^6.0.2",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "@types/styled-components": "^5.1.34",
    "@types/styled-components-react-native": "^5.2.5",
    "axios": "^1.7.2",
    "babel-plugin-styled-components": "^2.1.4",
    "expo": "~51.0.19",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.20",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-random": "~14.0.1",
    "expo-router": "~3.5.18",
    "expo-screen-orientation": "~7.0.5",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-pager-view": "6.3.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.1.0",
    "react-native-web": "^0.19.12",
    "styled-components": "^6.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.3.3"
  },
  "private": true
}
